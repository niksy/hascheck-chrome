{
    "name": "__MSG_appName__",
    "version": "0.1.2",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "permissions": [
        "contextMenus",
        "activeTab",
        "*://*/*"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "hr",
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "web_accessible_resources": [
        "images/*.png",
        "images/*.svg"
    ]
}
